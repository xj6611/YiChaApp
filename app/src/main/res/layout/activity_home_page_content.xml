<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_my__zt__he_zuo_"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="#ffffff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/Table_RelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44">
            <FrameLayout
                android:id="@+id/back_Fl"
                android:layout_width="40.0dip"
                android:layout_height="60.0dip"
                android:layout_centerVertical="true">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@mipmap/left" />
            </FrameLayout>
            <TextView
                android:id="@+id/Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_10"
                android:text="估价系统"
                android:textColor="@color/A333333"
                android:textSize="@dimen/sp_17"
                android:layout_centerInParent="true"/>

            <FrameLayout
                android:id="@+id/Add_FrameLayout"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="搜索"
                    android:textSize="@dimen/sp_12"
                    android:drawablePadding="@dimen/dp_2"
                    android:textColor="@color/A999999"
                    android:drawableTop="@mipmap/map_search_gray"/>
            </FrameLayout>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="#ffededed" />

        <android.support.design.widget.TabLayout
            android:id="@+id/MyShouCang__tb_title"
            android:layout_width="fill_parent"
            android:layout_height="45.0dip"
            android:layout_gravity="center"
            app:tabGravity="fill"
            app:tabIndicatorColor="#ff4f8ded"
            app:tabIndicatorHeight="2.0dip"
            app:tabMaxWidth="0.0dip"
            app:tabMode="fixed"
            app:tabSelectedTextColor="#ff508cee"
            app:tabTextColor="#ff999999" />

        <View
            android:id="@+id/View1"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="#ffededed" />

        <android.support.v4.view.ViewPager
            android:id="@+id/MyShouCang_ViewPager"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" />
    </LinearLayout>

</LinearLayout>