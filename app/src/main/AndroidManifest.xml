        android:roundIcon="@mipmap/touciang"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.TheStoreSearchActivity" />
        <activity android:name=".Activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PC_DatatlisActivity" />
        <activity android:name=".Activity.HomePageContentActivity" />
        <activity android:name="com.dyc.piclibrary.activity.PickOrTakeImageActivity" />
        <activity android:name="com.dyc.piclibrary.activity.CropImgActivity" />
        <activity android:name=".Activity.ForgotPasswordActivity" />
        <activity android:name=".Activity.ResetYourPasswordActivity" />
        <activity android:name=".Activity.PC_ValuationActivity" />
        <activity android:name=".Activity.Phone_DetalisActivity" />
        <activity android:name=".Activity.Phone_ValuationActivity"></activity>
        <activity android:name=".Activity.Card_DatatlisActivity"/>
